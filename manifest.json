{
	"name":            "InterfaceLIFT Wallpapers",
	"version":         "1.0",
	"manifest_version": 2,

	"description": "Shows latest InterfaceLIFT wallpaper on your speed dial.",
	"developer": {
		"name": "RunninglVlan",
		"url":  "https://github.com/RunninglVlan"
	},
	"icons": {
		"64":  "img/icon_64.png",
		"128": "img/icon_128.png"
	},

	"permissions": ["https://*/*", "speeddial"],
	"speeddial": {
		"title": "Loading...",
		"url":   "https://interfacelift.com/"
	},
	"background": {
		"page": "speeddial.html"
	}
}
